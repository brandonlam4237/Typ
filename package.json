        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.11.0",
        "sass": "^1.62.1",
        "sequelize": "^6.31.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3",
        "vite-express": "*"
    },
    "devDependencies": {
        "@types/express": "^4.17.15",
        "@types/node": "^18.11.18",
